load("@root//buck/shims:shims.bzl", "shims")

shims.constraint(
    name="target",
    values=[
        "x86_64-unknown-uefi",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-uefi",
        "aarch64-unknown-linux-gnu",
        "aarch64-apple-darwin",
    ],
)
